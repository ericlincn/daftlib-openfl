			var arr:Array<String> = charArr[i].split(":");
			var index:Int = Std.parseInt(arr[0]);
			if(index == code)
			{
				arr = arr[1].split(",");

				var bmd:BitmapData = new BitmapData(Std.parseInt(arr[0]), Std.parseInt(arr[1]), true, 0xffffff);
				var j:Int = 2;
				while(j < arr.length)
				{
					bmd.setPixel32(Std.parseInt(arr[j]), Std.parseInt(arr[j + 1]), color32);
					j += 2;
				}
				return bmd;
			}

			i++;
		}

		return null;
	}
	public function getBitmapDataByChar(char:String, color32:Int):BitmapData
	{
		if(char == null || char.length == 0 || char.length > 1)
			return null;

		return getBitmapDataByCharcode(char.charCodeAt(0), color32);
	}
}